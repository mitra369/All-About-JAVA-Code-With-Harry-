package com.company;
import java.util.Scanner;

class Employee{
    private int id;
   private String name;

//Constructor initialization
    public Employee(){
        id=210112;
        name="Antu Mitra";
    }
   //public void setid(int d) { id=d;}   //No need because of constructor. Constructor are capable od doing this shit.
   //public void setname(String n){n=name; }
     public int getid(){ return id;}
     public String getname(){ return name;}

}
public class Constructor {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        Employee antu= new Employee();

        System.out.println(antu.getid());
        System.out.println(antu.getname());

    }
}
