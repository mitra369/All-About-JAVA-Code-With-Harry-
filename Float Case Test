import java.util.Scanner;
public class Float_case_test {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int a= sc.nextInt();

        //Boolean function
        System.out.println(a>10);

        int b= sc.nextInt();
         float f= (a/(b*1.00f));
        System.out.println(f);
    }
}
